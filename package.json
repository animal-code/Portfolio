{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "2020 Portfolio",
  "author": "seungju <kingsky32@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0 --disableHostCheck true --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js"
  },
  "dependencies": {
    "axios": "~0.18.0",
    "bootstrap": "~4.3.1",
    "bootstrap-vue": "2.0.0-rc.16",
    "c3": "^0.6.7",
    "chart.js": "~2.8.0",
    "echarts": "^4.2.1",
    "idle-vue": "~2.0.5",
    "ladda": "~2.0.1",
    "moment": "~2.24.0",
    "nouislider": "~13.1.4",
    "numeral": "~2.0.6",
    "perfect-scrollbar": "~1.4.0",
    "plyr": "~2.0.18",
    "simplemde": "~1.11.2",
    "spinkit": "~1.2.5",
    "sweet-modal-vue": "~2.0.0",
    "text-mask-addons": "~3.8.0",
    "v-img": "~0.2.0",
    "vue": "~2.6.6",
    "vue-awesome-swiper": "~3.1.3",
    "vue-blockui": "~1.1.6",
    "vue-c3": "~1.2.9",
    "vue-chartjs": "~3.4.0",
    "vue-clipboard2": "~0.3.0",
    "vue-color": "~2.7.0",
    "vue-context-menu": "~2.0.6",
    "vue-cropper": "~0.4.0",
    "vue-dragula": "~1.3.1",
    "vue-echarts": "~4.0.1",
    "vue-flatpickr-component": "~8.1.2",
    "vue-form-wizard": "~0.8.4",
    "vue-gallery": "~2.0.0",
    "vue-input-tag": "~2.0.5",
    "vue-jstree": "~2.1.6",
    "vue-masonry": "~0.11.1",
    "vue-meta": "~1.6.0",
    "vue-multiselect": "~2.1.0",
    "vue-notification": "~1.3.7",
    "vue-number-input-spinner": "~2.1.0",
    "vue-password-strength-meter": "~1.4.2",
    "vue-quill-editor": "~3.0.5",
    "vue-router": "~3.0.1",
    "vue-simple-calendar": "~4.2.1",
    "vue-simplemde": "~0.5.1",
    "vue-slider-component": "~3.0.16",
    "vue-tables-2": "~1.4.50",
    "vue-text-mask": "~6.1.1",
    "vue-toasted": "~1.1.24",
    "vue-typeahead": "~2.3.2",
    "vue-upload-component": "~2.8.3",
    "vue2-dropzone": "3.5.2",
    "vue2-google-maps": "~0.10.2",
    "vuedraggable": "2.20.0",
    "vuejs-datepicker": "~1.5.3",
    "vuetrend": "~0.3.2",
    "zxcvbn": "~4.4.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "node-sass": "~4.11.0",
    "rename-webpack-plugin": "^2.0.0",
    "sass-loader": "^7.1.0",
    "suppress-chunks-webpack-plugin": "^1.0.5",
    "vue-template-compiler": "~2.6.6",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-jest": "^23.6.0"
  },
  "resolutions": {
    "sass-loader/node-sass": "~4.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "/dist/",
    "/*.js",
    "/test/unit/coverage/",
    "src/vendor/libs/**/*.js"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/test/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
